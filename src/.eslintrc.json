{
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "globals": {
    "React": "readonly"
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["babel"],
  "extends": ["plugin:react/recommended", "plugin:react-hooks/recommended"],
  "rules": {
    "valid-typeof": 0,
    "babel/valid-typeof": 1
  }
}
